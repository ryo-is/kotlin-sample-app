<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <Button
            android:text="@string/test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="148dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="148dp"
            android:layout_marginBottom="39dp" app:layout_constraintBottom_toTopOf="@+id/textView"
            android:layout_marginTop="165dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.0" android:elevation="9dp"
            app:layout_constraintVertical_bias="1.0"/>
    <TextView
            android:text="@string/textviewarea"
            android:layout_width="166dp"
            android:layout_height="66dp"
            android:id="@+id/textView"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="222dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="163dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="163dp"
            android:layout_marginTop="39dp" app:layout_constraintTop_toBottomOf="@+id/button"/>

</android.support.constraint.ConstraintLayout>